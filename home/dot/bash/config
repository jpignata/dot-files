complete -C '/usr/local/aws/bin/aws_completer' aws

. ~/.bash/git/git-completion.bash
. ~/.bash/git/git-prompt.sh

EDITOR='nvim'
VISUAL='nvim'
GIT_PS1_SHOWDIRTYSTATE=true
PS1='\w$(__git_ps1 "(%s)")\$ '
PATH=/bin:/usr/bin:/usr/local/bin:/opt/local/bin:/opt/local/sbin:/sbin:/usr/sbin:/usr/local/sbin:/usr/X11/bin:~/bin

export EDITOR VISUAL GIT_PS1_SHOWDIRTYSTATE PS1 PATH
